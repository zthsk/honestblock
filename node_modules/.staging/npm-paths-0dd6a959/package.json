{
  "name": "npm-paths",
  "description": "Returns an array of unique \"npm\" directories based on the user's platform and environment.",
  "version": "0.1.3",
  "homepage": "https://github.com/jonschlinkert/npm-paths",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/npm-paths",
  "bugs": {
    "url": "https://github.com/jonschlinkert/npm-paths/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "global-modules": "^0.2.0",
    "is-windows": "^0.1.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "helper-example": "^0.1.0",
    "mocha": "*"
  },
  "keywords": [
    "directory",
    "dirname",
    "global",
    "module",
    "package",
    "path",
    "prefix",
    "resolve"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "resolve-up",
        "look-up",
        "global-modules",
        "global-prefix",
        "global-paths",
        "contains-path"
      ]
    },
    "reflinks": [
      "global-paths",
      "verb"
    ],
    "lint": {
      "reflinks": true
    },
    "helpers": [
      "helper-example"
    ]
  }

,"_resolved": "https://registry.npmjs.org/npm-paths/-/npm-paths-0.1.3.tgz"
,"_integrity": "sha1-a1jnpHZjZrVD4KRu/kEVmo8GoH8="
